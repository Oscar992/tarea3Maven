<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" verbose="2" time-out="60000">
    <groups>
        <run>
            <include name="regression"/>
        </run>
    </groups>

    <listeners>
        <listener class-name="listeners.TestListeners"></listener>
        <listener class-name="listeners.SuiteListeners"></listener>
    </listeners>

    <test name="testPokemon">
        <packages>
            <package name="test.pokemon"/>
        </packages>
    </test>

    <test name="testPersona">
        <packages>
            <package name="test.persona"/>
        </packages>
    </test>

</suite>